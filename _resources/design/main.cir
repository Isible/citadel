# `*` means that the value is a pointer
@myFunc($someArg *i32) i32 {
    $foo lcl i8 = l{59}
    # `^` to create a pointer
    $bar lcl *i8 = ^foo
    # `~` to dereference a pointer
    ret: ~someArg
}